pipeline {

    agent any
    tools {
        maven 'Maven_3.8.1' 
    }
    stages {
        stage('Compile stage') {
            steps {
                bat "cd DailySharePrice && mvn clean compile" 
        }
    }

         stage('testing stage') {
             steps {
                bat "cd DailySharePrice && mvn test"
        }
    }

          stage('deployment stage') {
              steps {
                bat "cd DailySharePrice && mvn deploy"
        }
    }

  }

}

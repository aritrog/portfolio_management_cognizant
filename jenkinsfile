pipeline {

    agent any
    tools {
        maven 'Maven_3.8.1' 
    }
    stages {
        stage('Compile stage') {
            steps {
             parallel(
                bat "mvn -fDailySharePrice/pom.xml clean compile",
                bat "mvn -fDailyMutualFundNAV/pom.xml clean compile"
             )
        }
    }

         stage('testing stage') {
             steps {
              parallel(
                bat "mvn -fDailySharePrice/pom.xml test",
                bat "mvn -fDailyMutualFundNAV/pom.xml test"
              )
        }
    }

          stage('package stage') {
              steps {
               parallel(
                bat "mvn -fDailySharePrice/pom.xml package",
                bat "mvn -fDailyMutualFundNAV/pom.xml package"
              )
        }
    }

  }

}


